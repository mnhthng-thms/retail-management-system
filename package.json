{
  "name": "retail-management-system",
  "version": "0.2.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build-webpack": "sudo webpack --config webpack.production.js --mode production",
    "prepare-dist": "sudo rm -r dist; mkdir dist && sudo cp -r src/. dist",
    "transpile-js": "babel src/. --out-dir js-for-dist",
    "transpile-for-dist": "yarn run prepare-dist; yarn run transpile-js; sudo cp -r js-for-dist/. dist/.; sudo rm -r js-for-dist",
    "build-dist": "yarn run transpile-for-dist; sudo cp package.json dist/.; cd dist; yarn install",
    "start": "NODE_ENV=production JWT_SECRET=infinitygaunlet node src/server.js",
    "dev": "DEBUG=App:API:* NODE_ENV=development nodemon src/server.js --exec babel-node",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
  },
  "author": "MinhTu Thomas Hoang",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "babel-loader": "^8.0.6",
    "bcrypt": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.2",
    "morgan": "^1.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "debug": "^4.1.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
